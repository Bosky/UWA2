DROP TABLE IF EXISTS "PEOPLE";
CREATE TABLE "PEOPLE" ("ID" INTEGER PRIMARY KEY  AUTOINCREMENT  NOT NULL  UNIQUE , "FIRSTNAME" TEXT NOT NULL , "LASTNAME" TEXT NOT NULL , "OFFICE" TEXT NOT NULL , "EMAIL" TEXT NOT NULL );
